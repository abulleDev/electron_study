    "@electron/packager": "^18.3.2",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^30.0.1",
    "electron-winstaller": "^5.4.0",
    "ts-node": "^10.9.2",
