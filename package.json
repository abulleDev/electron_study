{
  "name": "electron_study",
  "version": "1.0.0",
  "private": true,
  "description": "An Electron project created for learning Electron",
  "main": "./dist/main/main.js",
  "scripts": {
    "dev": "tsc && ts-node ./script/dev.ts",
