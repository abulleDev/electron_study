{
  "name": "electron_study",
  "version": "1.0.0",
  "private": true,
  "description": "An Electron project created for learning Electron",
  "main": "./dist/main/main.js",
  "scripts": {
    "dev": "tsc && ts-node ./script/dev.ts",
    "build": "vite build ./app && tsc && ts-node ./script/build.ts",
    "build:win": "electron-packager . --platform=win32 --arch=x64,arm64 --asar --out=out",
    "build:mac": "electron-packager . --platform=darwin --arch=x64,arm64 --asar --out=out",
    "build:linux": "electron-packager . --platform=linux --arch=x64,arm64 --asar --out=out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abulleDev/electron_study.git"
  },
  "author": "abulleDev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abulleDev/electron_study/issues"
  },
  "homepage": "https://github.com/abulleDev/electron_study#readme",
  "devDependencies": {
    "@electron/packager": "^18.3.2",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^9.0.1",
    "electron": "^30.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  }
}
